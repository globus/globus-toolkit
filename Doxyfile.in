PROJECT_NAME           = "Globus Toolkit"
PROJECT_NUMBER         = @VERSION@
OPTIMIZE_OUTPUT_FOR_C  = YES
GENERATE_MAN           = YES
MAN_LINKS              = YES
GENERATE_LATEX         = NO
GENERATE_HTML          = YES
HTML_TIMESTAMP         = NO
OUTPUT_DIRECTORY       = doc
HIDE_UNDOC_MEMBERS     = YES
HIDE_UNDOC_CLASSES     = YES
HIDE_UNDOC_RELATIONS   = YES
GENERATE_DEPRECATEDLIST= NO
RECURSIVE              = YES
QUIET                  = YES
PREDEFINED             = GLOBUS_GLOBAL_DOCUMENT_SET GLOBUS_DONT_DOCUMENT_INTERNAL DOXYGEN
EXCLUDE_PATTERNS       = version.h
INPUT                  = doxygen @GT_DOXYGEN_SUBDIRS@ gram/protocol/source gram/rsl/source
INPUT_FILTER           = @srcdir@/globus-vararg-enums-doxygen-filter.pl
EXAMPLE_PATH           = @GT_DOXYGEN_SUBDIRS@
EXAMPLE_PATTERNS       = *.example

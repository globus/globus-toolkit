+#define FAIL(X) if (X) goto fail
+#define FAILEX(X, ...) if (X) {error(__VA_ARGS__); goto fail;}
+#define SSPI_FAIL(X) if ((sspiCode = (X)) != SEC_E_OK) goto fail

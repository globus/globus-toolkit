+  strcat(pw_shellpath, "\\cmd.exe");
+  
+  debug3("getpwnam: shell path [%s]", pw_shellpath);
+
+  /*
+   * Copy user name to static structure.
+   */
+  
+  strncpy(pw_username, userin, UNLEN + 1);
+
+  /*
+   * Get a token for this user.
+   */
+  
+  return &pw;
+}

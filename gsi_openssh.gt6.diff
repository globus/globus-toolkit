+/*
+ * Hardcoded, kerberos5 OID in <type><len><OID> format.
+ */
+  
+static unsigned char KRB5_OID[] =
+{
+  SSH_GSS_OIDTYPE,
+  9,
+  0x2a, 0x86, 0x48, 0x86, 0xf7, 0x12, 0x01, 0x02, 0x02
+};

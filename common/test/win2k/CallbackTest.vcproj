<?xml version="1.0"?>
<VisualStudioProject
	ProjectType="Visual C++"
	Version="7.00"
	Name="CallbackTest">
	<Build>
		<Settings>
			<Platform
				Name="Win32"/>
			<Configuration
				Name="Debug|Win32"
				IntermediateDirectory="Build"
				OutputDirectory="Build"
				ConfigurationType="1"
				CharacterSet="2">
				<Tool
					Name="VCBscMakeTool"/>
				<Tool
					Name="VCCLCompilerTool"
					Optimization="0"
					IncludePaths="$(GLOBUS_LOCATION)\include\"
					Defines="WIN32;_DEBUG;_CONSOLE"
					MinimalRebuild="TRUE"
					FullRuntimeChecks="TRUE"
					RuntimeLibrary="5"
					UsePrecompiledHeader="0"
					WarnLevel="3"
					SuppressStartupMessage="TRUE"
					DebugInformation="4"/>
				<Tool
					Name="VCCustomBuildTool"/>
				<Tool
					Name="VCLinkerTool"
					AdditionalInputs="kernel32.lib user32.lib gdi32.lib winspool.lib comdlg32.lib advapi32.lib shell32.lib ole32.lib oleaut32.lib uuid.lib odbc32.lib odbccp32.lib globus_common.lib Ws2_32.lib"
					OutputFile="$(OutDir)/CallbackTest.exe"
					LinkIncremental="2"
					SuppressStartupMessage="TRUE"
					LibraryPaths="$(GLOBUS_LOCATION)\lib\"
					GenerateDebug="TRUE"
					ProgramDatabaseFile="$(OutDir)/TestCommon.pdb"
					SubSystem="1"
					TargetMachine="1"/>
				<Tool
					Name="VCMIDLTool"
					TargetEnvironment="1"/>
				<Tool
					Name="VCPostBuildEventTool"/>
				<Tool
					Name="VCPreBuildEventTool"/>
				<Tool
					Name="VCPreLinkEventTool"/>
				<Tool
					Name="VCResourceCompilerTool"/>
				<Tool
					Name="VCSDLTool"/>
			</Configuration>
			<Configuration
				Name="Release|Win32"
				IntermediateDirectory="Release"
				OutputDirectory="Release"
				ConfigurationType="1"
				CharacterSet="2">
				<Tool
					Name="VCBscMakeTool"/>
				<Tool
					Name="VCCLCompilerTool"
					Optimization="2"
					InlineFunctionExpansion="1"
					FramePointerOmission="TRUE"
					Defines="WIN32;NDEBUG;_CONSOLE"
					StringPooling="TRUE"
					RuntimeLibrary="4"
					PackageFunctions="TRUE"
					UsePrecompiledHeader="3"
					WarnLevel="3"
					SuppressStartupMessage="TRUE"
					DebugInformation="3"/>
				<Tool
					Name="VCCustomBuildTool"/>
				<Tool
					Name="VCLinkerTool"
					AdditionalInputs="kernel32.lib user32.lib gdi32.lib winspool.lib comdlg32.lib advapi32.lib shell32.lib ole32.lib oleaut32.lib uuid.lib odbc32.lib odbccp32.lib"
					OutputFile="$(OutDir)/callbackTest.exe"
					LinkIncremental="1"
					SuppressStartupMessage="TRUE"
					GenerateDebug="FALSE"
					SubSystem="1"
					TargetMachine="1"/>
				<Tool
					Name="VCMIDLTool"
					TargetEnvironment="1"/>
				<Tool
					Name="VCPostBuildEventTool"/>
				<Tool
					Name="VCPreBuildEventTool"/>
				<Tool
					Name="VCPreLinkEventTool"/>
				<Tool
					Name="VCResourceCompilerTool"/>
				<Tool
					Name="VCSDLTool"/>
			</Configuration>
		</Settings>
	</Build>
	<Files>
		<Filter
			Name="Source Files"
			Filter="cpp;c;cxx;rc;def;odl;idl;hpj;bat;asm">
			<File
				RelativePath="..\globus_common_poll_test.c">
			</File>
		</Filter>
	</Files>
	<Globals>
	</Globals>
</VisualStudioProject>
